{
  "name": "blanket",
  "description": "seamless js code coverage",
  "version": "1.1.7",
  "homepage": "https://github.com/wayfarer247/blanket",
  "author": {
    "name": "Alex-Seville, Ethan Mick",
    "email": "ethan@ethanmick.com",
    "url": "http://blanketjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wayfarer247/blanket.git"
  },
  "bugs": {
    "url": "https://github.com/wayfarer247/blanket/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/wayfarer247/blanket/blob/master/LICENSE-MIT"
    }
  ],
  "main": "src/index.js",
  "engines": {
    "node": ">=0.10.x"
  },
  "dependencies": {
    "esprima": "1.2.2",
    "falafel": "0.3.1",
    "xtend": "~3.0.0"
  },
  "devDependencies": {
    "async": "~0.9.0",
    "coffee-script": "1.8.0",
    "mocha": "~1.20.1",
    "travis-cov": "*"
  },
  "scripts": {
    "test": "make test; make travis-cov"
  },
  "config": {
    "blanket": {
      "pattern": "test",
      "data-cover-flags": {
        "debug": false
      },
      "loader": "./node-loaders/coffee-script",
      "data-cover-reporter-options": {
        "shortnames": true
      }
    },
    "travis-cov": {
      "threshold": 70,
      "removeKey": "branchFcn"
    }
  },
  "keywords": [
    "node-blanket",
    "code coverage",
    "code",
    "coverage"
  ]
}
